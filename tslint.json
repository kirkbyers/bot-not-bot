{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": {
      "options": [
        "single",
        "double"
      ]
    },
    "no-console": {
      "severity": "warn"
    },
    "interface-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}